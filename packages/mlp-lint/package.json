{
  "name": "mlp-lint",
  "version": "0.0.1",
  "description": "multiple lint package cli",
  "keywords": [
    "cli",
    "multiple-lint-package",
    "eslint",
    "stylelint",
    "markdownlint",
    "commitlint"
  ],
  "author": "yanlei <yanlei@micronet.ltd>",
  "homepage": "https://github.com/ygood/multiple-lint-pkg#readme",
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": "./lib/cli.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "npm run copyfiles && tsc -w",
    "build": "rm -rf lib && npm run copyfiles && tsc",
    "copyfiles": "copyfiles -a -u 1 \"src/config/**\" lib",
    "test": "npm run build && jest",
    "coverage": "nyc jest --silent --forceExit",
    "prepublishOnly": "npm run test",
    "lint": "eslint src/ --ext ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmmirror.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ygood/multiple-lint-pkg.git"
  },
  "bugs": {
    "url": "https://github.com/ygood/multiple-lint-pkg/issues"
  },
  "devDependencies": {
    "@types/command-exists": "1.2.0",
    "@types/cross-spawn": "6.0.2",
    "@types/ejs": "^3.1.2",
    "@types/eslint": "^8.44.2",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.6",
    "@types/prettier": "^3.0.0",
    "@types/stylelint": "^14.0.0",
    "@types/text-table": "^0.2.2",
    "copyfiles": "^2.4.1",
    "jest": "^29.6.3",
    "nyc": "^15.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@babel/core": "^7.22.11",
    "@babel/eslint-parser": "^7.22.11",
    "@babel/preset-react": "^7.22.5",
    "@commitlint/cli": "^17.7.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "chalk": "4.1.2",
    "command-exists": "^1.2.9",
    "commander": "^11.0.0",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.9",
    "eslint": "^8.47.0",
    "eslint-config-egg": "^12.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "inquirer": "8.0.0",
    "husky": "^8.0.3",
    "is-docker": "^3.0.0",
    "lodash": "^4.17.21",
    "markdownlint": "^0.30.0",
    "markdownlint-rule-helpers": "^0.21.0",
    "ora": "^7.0.1",
    "prettier": "^3.0.2",
    "strip-ansi": "^7.1.0",
    "stylelint": "^15.10.3",
    "stylelint-scss": "^5.1.0",
    "terminal-link": "^3.0.0",
    "text-table": "^0.2.0",
    "vue-eslint-parser": "^9.3.1",
    "mlp-commitlint-config": "workspace:^",
    "mlp-eslint-config": "workspace:^",
    "mlp-markdownlint-config": "workspace:^",
    "mlp-stylelint-config": "workspace:^"
  }
}
